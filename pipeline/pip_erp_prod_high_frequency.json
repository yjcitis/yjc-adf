	"name": "pip_erp_prod_high_frequency",
	"properties": {
		"description": "ERP 產品",
		"activities": [
			{
				"name": "CopyINVMF",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "CopyCMSMA",
						"dependencyConditions": [
							"Completed"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"additionalColumns": [
							{
								"name": "ETL_DATE",
								"value": {
									"value": "@addhours(utcnow(),8)",
									"type": "Expression"
								}
							}
						],
						"sqlReaderQuery": {
							"value": "@{concat(\n    'select * from INVMF WHERE MF003 >= (\n      SELECT DATEADD(MONTH, -2, MAX(MA013))\n      FROM CMSMA\n) ')}",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": {
							"value": "@{concat(\n    'DELETE FROM erp.INVMF\n      WHERE MF003 >= (\n      SELECT DATEADD(MONTH, -2, MAX(MA013))\n      FROM erp.CMSMA\n) ')}",
							"type": "Expression"
						},
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"COMPANY",
								"MF001",
								"MF002",
								"MF003",
								"MF004",
								"MF005",
								"MF006",
								"MF007",
								"MF008"
							]
						},
						"sqlWriterUseTableLock": true,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "INVMF",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ERP_INVMF",
						"type": "DatasetReference"
					}
				]
			},
