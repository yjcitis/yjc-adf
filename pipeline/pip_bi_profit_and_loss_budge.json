{
	"name": "pip_bi_profit_and_loss_budge",
	"properties": {
		"activities": [
			{
				"name": "Copy_PROFIT_AND_LOSS_BUDGE",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"additionalColumns": [
							{
								"name": "ETL_DATE",
								"value": {
									"value": "@addhours(utcnow(),8)",
									"type": "Expression"
								}
							}
						],
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"year",
								"month",
								"account_id"
							]
						},
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "PROFIT_AND_LOSS_BUDGET",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DBO_PROFIT_AND_LOSS_BUDGET",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "PIP_ODS/BI"
		},
		"annotations": []
	}
}